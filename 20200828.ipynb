   "source": [
    "1.Function to print all numbers divisible by 6 and not a factor of 100 in a given range(lb, ub) inclusive\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "6\n",
      "12\n",
      "18\n",
      "24\n",
      "30\n",
      "36\n",
      "42\n",
      "48\n"
     ]
    }
   ],
   "source": [
    "def fun_6(lb,ub):\n",
    "    for i in range(lb,ub+1):\n",
    "        if ((i%6==0)and(i%100!=0)):\n",
    "            print(i)\n",
    "fun_6(1,50)\n"
   ]
