    "9.Function to generate N odd armstrong numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def odd_armstrong(n):\n",
    "    s=0\n",
    "    temp=n\n",
    "    while(n!=0):\n",
    "        r=n%10\n",
    "        s=s+r*r*r\n",
    "        n=n/10\n",
    "    if(s==temp):\n",
    "        if(s%2!=0):\n",
    "            return True\n",
    "    else:\n",
    "        return False\n",
    "def pri_arm(N):\n",
    "    c=0\n",
    "    start=1\n",
    "    numd=[]\n",
    "    while(True):\n",
    "        if odd_armstrong(start):\n",
    "            c+=1\n",
    "            numd.append(start)\n",
    "        if c==N:\n",
    "            break\n",
    "        else:\n",
    "            start+=1\n",
    "    return numd\n",
    "numd=pri_arm(3)\n",
    "print(numd)\n",
    "\n"
   ]
