   "source": [
    "8.Function to calculate average of all factorials in a given range\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "403791.3\n"
     ]
    }
   ],
   "source": [
    "def factorial(n):\n",
    "    fact=1\n",
    "    for i in range(1,n+1):\n",
    "        fact=fact*i\n",
    "    return fact\n",
    "def avg_fact(N):\n",
    "    fact_sum=0\n",
    "    for j in range(1,N+1):\n",
    "        k=factorial(j)\n",
    "        fact_sum=fact_sum+k\n",
    "    avg=fact_sum/N\n",
    "    print(avg)\n",
    "avg_fact(10)\n"
   ]
